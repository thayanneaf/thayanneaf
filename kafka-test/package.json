{
  "name": "kafka-test",
  "version": "1.0.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "cy:test": "cypress run --config video=false --env allure=true",
    "cy:open": "cypress open",
    "cy:dash": "npx cypress run --record --key {secrets_dash}",
    "allure:report": "allure generate --clean allure-results && allure open"
