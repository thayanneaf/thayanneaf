  "keywords": [
    "cypress",
    "tests",
    "kafkaJs",
    "KafkaNode",
    "github-actions"
  ],
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.40.1",
    "cypress": "^13.6.2",
    "cypress-wait-until": "^1.7.2",
    "kafka-node": "5.0.0",
    "kafkajs": "2.2.0"
  }
}
